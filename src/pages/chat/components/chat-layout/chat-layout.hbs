<main class='chat-layout'>
    <aside class='chat-layout__side-content'>
        <div class="chat-layout__profile">
            {{> Link href="#" data-page="/profile" text="Профиль" class="link-secondary link-m"}}
            <img src="/navigate.svg">
        </div>
        <form>
            <input class="chat-layout__search-field" placeholder="Поиск">
        </form>
        <div class="chat-layout__chats_feed">
            {{> chats_feed}}
        </div>
    </aside>
    <section class='chat-layout__chat'>
        {{> chat}}
    </section>
</main>


